<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="Theme.MaterialComponents.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/sageResearchPrimary</item>
        <item name="colorPrimaryDark">@color/sageResearchPrimaryDark</item>
        <item name="colorAccent">@color/sageResearchSecondary</item>
        <item name="colorControlHighlight">@color/ocean100</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/AppTheme.WindowTransition</item>
    </style>

</resources>