<?xml version="1.0" encoding="utf-8"?>

<rotate xmlns:android="http://schemas.android.com/apk/res/android"
        android:fromDegrees="-90"
        android:toDegrees="-90">
<shape
        android:innerRadiusRatio="@dimen/countdownDialInnerRadiusRatio"
        android:shape="ring"
        android:thicknessRatio="@dimen/countdownDialThicknessRatio"
        android:useLevel="true">
    <gradient
            android:centerY=".5"
            android:endColor="@color/royal500"
            android:startColor="@color/royal500"
            android:type="sweep"
            android:useLevel="false" />
</shape>
</rotate>
